{
   "name": "elogank/lol-replay-observer-silex",
   "description": "A replay spectator (observer) server for League of Legend on the Silex micro-framework",
   "keywords": [
       "lol", "silex", "league of legends", "replay", "record", "spectator", "server", "observer", "download"
   ],
   "license": "MIT",
   "authors": [
       {
           "name": "Sylvain Lorinet",
           "email": "sylvain.lorinet@gmail.com"
       }
    ],
    "require": {
        "php": ">=5.4.0",

        "elogank/lol-replay-observer": "dev-master as 1.0.0",
        "elogank/lol-replay-downloader": "dev-master",
        "elogank/lol-replay": "dev-master",

        "silex/silex": "~1.3",
        "monolog/monolog": "1.9",
        "predis/predis": "~1.0"
    },
    "require-dev": {
        "symfony/var-dumper": "~2.7",
        "letudiant/composer-shared-package-plugin": "~2.0"
    },
    "extra": {
        "shared-package": {
            "vendor-dir": "../composer-dependencies",
            "package-list": [
                "elogank/*"
            ],
            "symlink-base-path": "F:\\Vagrant\\projects\\composer-dependencies"
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    }
}
